{
    "agent": "RNNAgent",
    "model": {
        "rnn_type": "lstm",
        "hidden_dim": 256,
        "encoding_dim": 16,
        "encoding_type": "learned",
        "num_layers": 1,
        "dropout": 0
    },
    "dataset": {
       "folder": "data",
        "train_split": "leave_one_out/train_leave_Expstu50.csv",
        "valid_split": "leave_one_out/test_leave_Expstu50.csv",
        "include_correctness": false,
        "pred_target": "nlg_score"
    },
    "dataloader": {
        "batch_size": 8,
        "num_workers": 0
    },
    "optimizer": {
        "learning_rate": 3e-4,
        "weight_decay": 0
    },
    "epochs": 300
}
